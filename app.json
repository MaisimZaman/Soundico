{
  "expo": {
    "name": "Soundico",
    "description": "test",
    "scheme": "soundico",
    "slug": "musiccoapp",
    "privacy": "public",
    "version": "0.0.4",
    "plugins": [
      [
        "expo-media-library",
        
        {
          "photosPermission": "Soundico would like to use your photo library to access and play your local audio files on the app",
          "savePhotosPermission": "Soundico would like to use your photo library to access and play your local audio files on the app",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "platforms": [
      "ios",
      "android"
    ],
    "githubUrl": "https://github.com/MaisimZaman/Free-Music-Soundify-",
    "orientation": "portrait",
    "icon": "./assets/SoundicoLogo.png",
    "splash": {
      "image": "./assets/Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#141414"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "NSPhotoLibraryUsageDescription": "Soundico would like to use your photo library to access and play your local audio files on the app",
        "usesAppleSignIn": true
        
      },
      "bundleIdentifier": "com.maisimdevelops.musiccoapp",
      "config": {

        "googleMobileAdsAppId": "ca-app-pub-1719409113112551~6399112888" 
      }
    },
    "android": {
      "package": "com.maisimdevelops.musiccoapp",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ],
      "config": {
       
        "googleMobileAdsAppId": "ca-app-pub-1719409113112551~9951345082"
      }
    }
  }
}
